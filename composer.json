{
  "name": "tourze/php-packer-generator",
  "description": "代码生成工具，用于PHP代码的合并、优化和格式控制",
  "type": "library",
  "require": {
    "php": "^8.1",
    "tourze/php-packer-ast": "0.0.*",
    "psr/log": "^3|^2|^1",
    "nikic/php-parser": "^5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PhpPacker\\Generator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpPacker\\Generator\\Tests\\": "tests/"
    }
  }
}